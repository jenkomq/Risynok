#include <stdio.h>

int task1() {
    
    int i, j;
    
    for(i = 1; i <= 10; i++) {
        for(j = 1; j <= 10; j++) {
            printf("%-4d", i * j);
        }
        printf("\n");
    }
    
    return 0;
}




#include <stdio.h>

int task2() {
    int i, j;
    
    for(i = 1; i <= 10; i++) {
        for(j = 1; j <= i; j++) {
            printf("%-4d", i * j);
        }
        printf("\n");
    }
    
    return 0;
}




#include <stdio.h>

int task3() {
    int i, j, num = 6;
    
    for(i = num; i >= 2; i--) {
        for(j = i; j >= 2; j--) {
            printf("%-4d", j);
        }
        printf("\n");
    }
    
    return 0;
}


void main()
{

	task3();
}
