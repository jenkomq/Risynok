#include <stdio.h>

int main()
{
    char c;
    
    printf("Введите символы 'a' or 'b': ");
    scanf("%c", &c);   // считывание с консоли

    if (c >= '0' && c <= '9') {   // проверка, является ли символ цифрой
        printf("Это цифра.");
    } else {
        printf("Это буква.");
    }
    
    return 0;
}
