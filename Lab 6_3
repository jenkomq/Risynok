#include <stdio.h>

int main() {
    int N, a, b, c, result;

    printf("Введите число N: ");
    scanf("%d", &N);

    // Разделение числа на цифры
    a = N / 100;
    b = (N / 10) % 10;
    c = N % 10;

    // Вычисление произведения цифр
    result = a * b * c;

    // Проверка на трехзначность произведения
    if (result >= 100 && result <= 999) {
        printf("Произведение цифр является трехзначным числом\n");
    } else {
        printf("Произведение цифр не является трехзначным числом\n");
    }

    return 0;
}
