
#include <stdio.h>
#include <math.h>

int main() {
    // Заданные значения переменных
    double x = 3.251;
    double y = 0.325;
    double z = 0.466 * pow(10, -4);

    // Расчет уравнения
    double result = ( pow(2, pow(y, x)) + pow(pow(3, x), y) ) - ( (y * (atan(z) - M_PI/6) ) / (fabs(x) + 1 / pow(y, 2) + 1 ));

    // Вывод результата
    printf("Результат: %.6lf\n", result);

    return 0;
}
