#include <stdio.h>

int main() {
    // В контрольном примере возьмем x = 5
    // При x <= 7, значение функции F(x) = 3x - 9
    int x = 5;
    int result1 = (x <= 7) ? 3*x - 9 : 1/(x*x) - 4;
    printf("F(%d) = %d\n", x, result1); // Вывод: F(5) = -6
    
    // В контрольном примере возьмем x = 9
    // При x > 7, значение функции F(x) = 1/x^2 - 4
    x = 9;
    int result2 = (x <= 7) ? 3*x - 9 : 1/(x*x) - 4;
    printf("F(%d) = %d\n", x, result2); // Вывод: F(9) = -3
    
    return 0;
}
