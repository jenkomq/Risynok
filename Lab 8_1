#include <stdio.h>

int main() {
    int n, m;
    int s = 0;

    printf("Введите m: ");
    scanf("%d", &m);

    printf("Введите n: ");
    scanf("%d", &n);

    for (int i = m; i <= n; i++) {
        s += i;
        printf("Выполнено %d раз\n", i - m + 1);
    }

    printf("Результат: %d\n", s);

    return 0;
}


/* Программа запрашивает у пользователя значения m и n, 
затем с помощью цикла for вычисляет сумму целых чисел от m до n (включительно),
выводя на каждой итерации номер текущей итерации. По завершении цикла выводится финальный результат.*/
